{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "dnsNamePrefix": {
      "value": "<DNS_PREFIX>-"
    },
    "agentCount": {
      "value": 3
    },
    "agentVMSize": {
      "value": "Standard_D2_v2"
    },
    "linuxAdminUsername": {
      "value": "azureuser"
    },
    "orchestratorType": {
      "value": "DCOS"
    },
    "masterCount": {
      "value": 1
    },
    "sshRSAPublicKey": {
      "value": "<YOUR_id_rsa.pub_DATA>"
    }
  }
}